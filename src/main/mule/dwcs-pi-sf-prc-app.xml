	<flow name="dwcs-pi-sf-prc-app-daily-flow" doc:id="513a0309-19fc-4315-b308-23143a902e14" >
		<scheduler doc:name="Scheduler" doc:id="05269f4c-3d6b-4af8-825e-f943405b5874" >
			<scheduling-strategy >
				<fixed-frequency />
			</scheduling-strategy>
		</scheduler>
