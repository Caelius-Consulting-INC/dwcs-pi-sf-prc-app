		<ee:transform doc:name="var : startDate &amp; endDate" doc:id="d4466dd3-1259-4515-b59e-4ac0015e5f19" >
			<ee:message >
			</ee:message>
			<ee:variables >
				<ee:set-variable resource="dwl/v-dailyStartDate.dwl" variableName="startDate" />
				<ee:set-variable resource="dwl/v-dailyEndDate.dwl" variableName="endDate" />
			</ee:variables>
		</ee:transform>
		<flow-ref doc:name="pi-iteration-sub-flow" doc:id="a43de2e3-77e1-43e8-bf0e-8b0d910463ba" name="pi-iteration-sub-flow"/>
